#include <bits/stdc++.h>
using namespace std;
#define ll long long
int d[1000005];
int main() {
   int t;cin>>t;
   while(t--){
   		int n;cin>>n;
   		int a[n];
        for(int i=1;i<=1000000;i++){
        	d[i]=0;
		}
   		for(int i=0;i<n;i++){
   			cin>>a[i];
   			d[a[i]]++;
	   }
	   for(int i=0;i<n;i++){
	   	if(d[a[i]]%2==1){
	   		cout<<a[i]<<endl;
	   		break;
		   } 
	   }
	}
   
}
